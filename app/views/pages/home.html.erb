                    <div class="mt-auto text-center">
                      <h5 class="font-weight-normal">Music Studio</h5>
                    </div>
                  </a>
                </div>

                <div class="js-slide my-3 lift-lg shadow-2-sm-hover">
                  <a class="w-100 card bg-img-hero border-0 gradient-overlay-half-black-v2 min-height-320 text-white rounded-pseudo p-3" href="#" style="background-image: url(https://images.unsplash.com/photo-1447758902204-48010b87c24d?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1050&q=80);">
                    <div class="mt-auto text-center">
                      <h5 class="font-weight-normal">Sculpting Studio</h5>
                    </div>
                  </a>
                </div>
              </div>
              <!-- End Slick Carousel -->
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- End Experiences -->


    <!-- Featured properties -->
    <div class="container space-2">
      <!-- Title -->
      <div class="row align-items-end mb-6">
        <div class="col-sm-8">
          <h2 class="text-lh-xs">Featured Spaces</h2>
          <p class="mb-sm-0">Curated creative spaces in your city.</p>
        </div>

