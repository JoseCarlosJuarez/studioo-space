          <a class="btn btn-primary rounded-pill btn-wide transition-3d-hover" href="#">See more spaces</a>
        </div>
      </div>
      <!-- End Title -->

      <div class="row">
          <div class="cards-grid p-3 flex-grow-1">
                <% @spaces.each do |space| %>
                <%= render '/spaces/card', space: space %>
                <% end %>
          </div>
      </div>
    </div>

